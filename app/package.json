{
  "name": "react-discussion-forum",
  "version": "1.0.0",
  "description": "A discussion forum built in React",
  "main": "src/index.js",
  "scripts": {
    "clean": "./clean.sh",
    "build:dev": "babel src -d views --watch",
    "start:dev": "NODE_ENV=development node index.js",
    "develop": "yarn clean && parallelshell 'yarn run build:dev' 'yarn run start:dev'",
    "start": "node index.js",
    "pack": "webpack --mode production --config webpack.config.js",
    "babel": "babel src -d views",
    "build": "yarn clean && yarn run pack && yarn run babel"
  },
  "author": "Nick Isaacs <nickisaacs@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.18.3",
    "parallelshell": "^3.0.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "express": "^4.16.3",
    "fela": "^6.1.9",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-fela": "^7.3.1",
    "react-hot-loader": "^4.3.4",
    "rimraf": "^2.6.2"
  }
}
